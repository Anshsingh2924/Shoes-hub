        // DOM elements
        const luxuryBrandsContainer = document.getElementById('luxury-brands');
        const sportsBrandsContainer = document.getElementById('sports-brands');
        const casualBrandsContainer = document.getElementById('casual-brands');
        const searchInput = document.getElementById('search-input');
        const searchBtn = document.getElementById('search-btn');
        const categorySelect = document.getElementById('category-select');
        const backToTopBtn = document.getElementById('backToTop');
        const homeBtn = document.getElementById('home-btn');
        const brandsBtn = document.getElementById('brands-btn');

        // Initialize the page
        document.addEventListener('DOMContentLoaded', function() {
            renderAllBrands();
            
            // Event listeners
            searchBtn.addEventListener('click', performSearch);
            searchInput.addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    performSearch();
                }
            });
            
            // Category filter
            categorySelect.addEventListener('change', filterByCategory);
            
            // Back to top button
            backToTopBtn.addEventListener('click', scrollToTop);
            window.addEventListener('scroll', toggleBackToTopButton);
            
            // Navigation buttons
            homeBtn.addEventListener('click', function() {
                showNotification("Redirecting to home page...");
            });
            
            brandsBtn.addEventListener('click', function() {
                showNotification("You're already on the brands page!");
            });
            
            // Navigation links
            document.querySelectorAll('.nav-links li').forEach(link => {
                link.addEventListener('click', function() {
                    document.querySelectorAll('.nav-links li').forEach(item => item.classList.remove('active'));
                    this.classList.add('active');
                    const categoryText = this.textContent.trim();
                    showNotification(`Exploring ${categoryText} brands`);
                });
            });
            
            // Footer links
            document.querySelectorAll('.footer-section ul li').forEach(link => {
                link.addEventListener('click', function() {
                    const linkText = this.textContent.trim();
                    showNotification(`Opening ${linkText} section`);
                });
            });
        });

        // Render all brands to the page
        function renderAllBrands() {
            // Clear containers
            luxuryBrandsContainer.innerHTML = '';
            sportsBrandsContainer.innerHTML = '';
            casualBrandsContainer.innerHTML = '';
            
            // Luxury brands
            brands.luxury.forEach(brand => {
                luxuryBrandsContainer.appendChild(createBrandCard(brand));
            });
            
            // Sports brands
            brands.sports.forEach(brand => {
                sportsBrandsContainer.appendChild(createBrandCard(brand));
            });
            
            // Casual brands
            brands.casual.forEach(brand => {
                casualBrandsContainer.appendChild(createBrandCard(brand));
            });
        }

        // Create a brand card element
        function createBrandCard(brand) {
            const card = document.createElement('div');
            card.className = `brand-card ${brand.badge}`;
            card.setAttribute('data-category', brand.category);
            card.setAttribute('data-name', brand.name.toLowerCase());
            
            const badgeClass = `${brand.badge}-badge`;
            const badgeHTML = `<div class="brand-badge ${badgeClass}">${brand.badge}</div>`;
            
            card.innerHTML = `
                ${badgeHTML}
                <div class="brand-logo-container">
                    <img src="${brand.logo}" alt="${brand.name} Logo" class="brand-logo-img">
                </div>
                <div class="brand-info">
                    <div class="brand-name">${brand.name}</div>
                    <div class="brand-origin"><i class="fas fa-globe"></i> ${brand.origin}</div>
                    <div class="brand-description">${brand.description}</div>
                    <div class="brand-stats">
                        <div class="stat-item">
                            <div class="stat-value">${brand.stats.founded}</div>
                            <div class="stat-label">Founded</div>
                        </div>
                        <div class="stat-item">
                            <div class="stat-value">${brand.stats.products}</div>
                            <div class="stat-label">Products</div>
                        </div>
                        <div class="stat-item">
                            <div class="stat-value">${brand.stats.stores}</div>
                            <div class="stat-label">Stores</div>
                        </div>
                    </div>
                    <a href="${brand.website}" target="_blank" class="brand-visit-btn" data-id="${brand.id}">
                        <i class="fas fa-external-link-alt"></i> Visit Official Site
                    </a>
                </div>
            `;
            
            return card;
        }

        // Search functionality
        function performSearch() {
            const searchTerm = searchInput.value.toLowerCase().trim();
            if (searchTerm === '') {
                showNotification('Please enter a brand name to search');
                return;
            }
            
            // Combine all brands into one array
            const allBrands = [...brands.luxury, ...brands.sports, ...brands.casual];
            const searchResults = allBrands.filter(brand => 
                brand.name.toLowerCase().includes(searchTerm) || 
                brand.origin.toLowerCase().includes(searchTerm) ||
                brand.description.toLowerCase().includes(searchTerm)
            );
            
            if (searchResults.length === 0) {
                showNotification(`No brands found for "${searchTerm}"`);
            } else {
                showNotification(`Found ${searchResults.length} brands for "${searchTerm}"`);
                highlightBrands(searchResults);
            }
        }

        function filterByCategory() {
            const selectedCategory = categorySelect.value;
            if (selectedCategory === 'all') {
                // Show all brands
                document.querySelectorAll('.brand-card').forEach(card => {
                    card.style.display = 'flex';
                });
                showNotification("Showing all brands");
            } else {
                // Filter by category
                document.querySelectorAll('.brand-card').forEach(card => {
                    if (card.getAttribute('data-category') === selectedCategory) {
                        card.style.display = 'flex';
                    } else {
                        card.style.display = 'none';
                    }
                });
                showNotification(`Showing ${selectedCategory} brands`);
            }
        }

        function highlightBrands(brandsToHighlight) {
            // First, show all brands
            document.querySelectorAll('.brand-card').forEach(card => {
                card.style.display = 'flex';
                card.style.opacity = '0.5';
                card.style.transform = 'scale(0.95)';
            });
            
            // Highlight matching brands
            brandsToHighlight.forEach(brand => {
                document.querySelectorAll(`.brand-card[data-name="${brand.name.toLowerCase()}"]`).forEach(card => {
                    card.style.opacity = '1';
                    card.style.transform = 'scale(1)';
                    card.style.boxShadow = '0 0 0 3px var(--primary)';
                    setTimeout(() => {
                        card.style.boxShadow = '';
                    }, 2000);
                });
            });
            
            // Reset opacity after 3 seconds
            setTimeout(() => {
                document.querySelectorAll('.brand-card').forEach(card => {
                    card.style.opacity = '1';
                    card.style.transform = '';
                });
            }, 3000);
        }

        // Back to top functionality
        function toggleBackToTopButton() {
            if (window.pageYOffset > 300) {
                backToTopBtn.classList.add('visible');
            } else {
                backToTopBtn.classList.remove('visible');
            }
        }

        function scrollToTop() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            });
        }

        // Notification system
        function showNotification(message) {
            // Create notification element
            const notification = document.createElement('div');
            notification.style.cssText = `
                position: fixed;
                top: 100px;
                right: 20px;
                background-color: var(--primary);
                color: white;
                padding: 15px 25px;
                border-radius: 8px;
                z-index: 10000;
                box-shadow: 0 5px 15px rgba(0,0,0,0.2);
                animation: slideIn 0.3s ease;
                font-weight: 600;
                max-width: 350px;
                font-size: 16px;
            `;
            
            notification.textContent = message;
            document.body.appendChild(notification);
            
            // Remove notification after 3 seconds
            setTimeout(() => {
                notification.style.animation = 'slideOut 0.3s ease';
                setTimeout(() => {
                    if (notification.parentNode) {
                        document.body.removeChild(notification);
                    }
                }, 300);
            }, 3000);
            
            // Add CSS for animation if not already added
            if (!document.getElementById('notification-styles')) {
                const style = document.createElement('style');
                style.id = 'notification-styles';
                style.textContent = `
                    @keyframes slideIn {
                        from { transform: translateX(100%); opacity: 0; }
                        to { transform: translateX(0); opacity: 1; }
                    }
                    @keyframes slideOut {
                        from { transform: translateX(0); opacity: 1; }
                        to { transform: translateX(100%); opacity: 0; }
                    }
                `;
                document.head.appendChild(style);
            }
        }

        // Track brand visits for analytics (simulated)
        let brandVisits = {};
        document.addEventListener('click', function(event) {
            if (event.target.closest('.brand-visit-btn')) {
                const brandLink = event.target.closest('.brand-visit-btn').href;
                const brandId = event.target.closest('.brand-visit-btn').getAttribute('data-id');
                
                // Simulate tracking
                if (!brandVisits[brandId]) {
                    brandVisits[brandId] = 0;
                }
                brandVisits[brandId]++;
                
                // Show notification
                const brandName = event.target.closest('.brand-card').querySelector('.brand-name').textContent;
                showNotification(`Opening ${brandName} official website...`);
                
                // In a real application, you would send this data to your analytics
                console.log(`Brand ${brandId} visited ${brandVisits[brandId]} times`);
            }
        });
    </script>
</body>
</html>
